<p id="notice"><%= notice %></p>

<h1>Listing Tb Eventos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Data</th>
      <th>Descricao</th>
      <th>Local</th>
      <th>Limite participantes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tb_eventos.each do |tb_evento| %>
      <tr>
        <td><%= tb_evento.nome %></td>
        <td><%= tb_evento.data %></td>
        <td><%= tb_evento.descricao %></td>
        <td><%= tb_evento.local %></td>
        <td><%= tb_evento.limite_participantes %></td>
        <td><%= link_to 'Show', tb_evento %></td>
        <td><%= link_to 'Edit', edit_tb_evento_path(tb_evento) %></td>
        <td><%= link_to 'Destroy', tb_evento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tb evento', new_tb_evento_path %>
