<%= simple_form_for(@winner) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :gift %>
    <%= f.association :user %>
  </div>

  <div class="form-group form-actions">
  	<%= link_to I18n.t('misc.action.back'), winners_path, class: 'btn btn-sm btn-square btn-default' %> 
    <%= f.button :submit, class: 'btn btn-sm btn-square btn-primary' %>
  </div>
<% end %>
